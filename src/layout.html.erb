<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="X-UA-Compatible" content="IE=edge"/>
	<title><%= title_string %></title>
	<style type="text/css" media="all">
	<%@stylesheets<<"application.css"%>
	<%for sheet in @stylesheets%>
	@import url('/stylesheets/<%=sheet%>');
	<%end%>
	</style>
	<%for js in @javascripts%>
	<script src="/javascripts/<%=js%>" type="text/javascript" charset="utf-8"></script>
	<%end%>
</head>
<body>
	<div id="container">
		<div id="main-header">
			<div class="title"><%=title_string%></div>
			<div id="navbar">
				<a href="/">Home</a>
				<a href="/gallery.html">Gallery</a>
			</div>
		</div>
		<%= subcontents %>
		<div id="license">
			<a rel="license" href="http://creativecommons.org/licenses/by-sa/3.0/deed.en_US" target="_blank"><img alt="Creative Commons License" style="border-width:0" src="http://i.creativecommons.org/l/by-sa/3.0/80x15.png" /></a><br />This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 Unported License.
		</div>
	</div>
</body>
</html>
